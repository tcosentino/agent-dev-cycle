{
  "name": "@agentforge/server",
  "version": "0.1.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "agentforge-server": "./src/cli.ts"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "start": "tsx src/cli.ts --services ../../src/services --storage sqlite --db ../../.data/agentforge.db",
    "dev": "tsx watch src/cli.ts --services ../../src/services --storage sqlite --db ../../.data/agentforge.db",
    "seed": "tsx src/seed.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@agentforge/dataobject": "0.1.0",
    "@hono/node-server": "^1.13.0",
    "@hono/zod-openapi": "^0.18.0",
    "better-sqlite3": "^11.0.0",
    "hono": "^4.6.0",
    "tsx": "^4.19.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^20.0.0",
    "vitest": "^2.0.0"
  }
}
