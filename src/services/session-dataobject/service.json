{
  "name": "session-dataobject",
  "type": "dataobject",
  "version": "0.1.0",
  "description": "Session data object. Records of agent execution sessions.",
  "entry": "index.ts",
  "schema": {
    "fields": [
      { "name": "id", "type": "uuid", "auto": true },
      { "name": "projectId", "type": "uuid", "required": true },
      { "name": "runId", "type": "string", "required": true },
      { "name": "agent", "type": "enum", "values": ["pm", "engineer", "qa", "lead"], "required": true },
      { "name": "phase", "type": "enum", "values": ["shaping", "building", "stabilizing"] },
      { "name": "summary", "type": "text" },
      { "name": "startedAt", "type": "date", "auto": true },
      { "name": "completedAt", "type": "date" }
    ]
  },
  "endpoints": [
    { "method": "GET", "path": "/sessions", "description": "List all sessions" },
    { "method": "GET", "path": "/sessions/:id", "description": "Get session by ID" },
    { "method": "GET", "path": "/projects/:projectId/sessions", "description": "List sessions for a project" },
    { "method": "POST", "path": "/sessions", "description": "Create a new session" },
    { "method": "PATCH", "path": "/sessions/:id", "description": "Update session (complete it)" }
  ],
  "dependencies": ["project-dataobject"],
  "tags": ["core", "agents"]
}
