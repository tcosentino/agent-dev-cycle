{
  "name": "agent-dev-cycle",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*",
    "example-projects/*"
  ],
  "scripts": {
    "dev": "concurrently -n ui,api -c blue,green \"vite\" \"yarn dev:server\"",
    "dev:ui": "vite",
    "dev:server": "yarn workspace @agentforge/server dev",
    "build": "tsc && vite build",
    "build:server": "yarn workspace @agentforge/server build",
    "build:all": "yarn build && yarn build:server",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "seed": "yarn workspace @agentforge/server seed",
    "test": "yarn workspace @agentforge/server test",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "concurrently": "^9.2.1",
    "playwright": "^1.57.0",
    "storybook": "^10.1.11",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "mermaid": "^11.12.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1"
  }
}
